{
  "name": "ngwp",
  "version": "2.0.10",
  "license": "MIT",
  "description": "Project scaffold (Webpack, AngularJS, ES6, SCSS, Jade/Pug)",
  "homepage": "https://github.com/DavidKk/ngwp",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidKk/ngwp.git"
  },
  "engines": {
    "node": ">= 6.2.2"
  },
  "scripts": {
    "build": "rimraf ./dist && babel ./src --out-dir ./dist",
    "eslint": "eslint ./src",
    "mocha": "mocha --require babel-polyfill --compilers js:babel-register ./unitest/*.spec.js",
    "test": "npm run eslint && npm run mocha && rimraf ./.temporary",
    "browser-test": "babel-node ./unitest/browser.js",
    "coverage": "rimraf ./coverage && nyc --reporter=text --reporter=lcov --report-dir=./coverage/cli npm run test",
    "coveralls": "nyc --reporter=text-lcov npm run test | coveralls",
    "start": "babel ./src --out-dir ./dist --watch"
  },
  "bin": {
    "ngwp": "./bin/ngwp"
  },
  "devDependencies": {
    "angular": "~1.5.6",
    "angular-mocks": "^1.5.8",
    "angular-ui-router": "^0.2.15",
    "babel-preset-es2015-maybe-webpack": "^1.2.1",
    "command-exists": "^1.2.2",
    "isparta-loader": "^2.0.0"
  },
