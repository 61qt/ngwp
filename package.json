    "babel-runtime": "^6.23.0",
    "browser-sync": "^2.15.0",
    "browser-sync-webpack-plugin": "^1.1.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "columnify": "^1.5.4",
    "commander": "^2.9.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "eslint": "^3.0.1",
