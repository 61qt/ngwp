{
  "name": "webpack-angular",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Project scaffold (Webpack, AngularJS, ES6, SCSS, Jade/Pug)",
  "homepage": "https://github.com/DavidKk/webpack-angular",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidKk/webpack-angular.git"
  },
  "engines": {
    "node": ">= 6.2.2"
  },
  "scripts": {
    "vhost": "./bin/vhosts",
    "builder": "./bin/module",
    "develop": "cross-env DEVELOP=1 webpack --color --debug --config conf/webpack.develop.config.babel.js --watch --delay 250ms",
    "product": "cross-env PRODUCT=1 webpack --progress --profile --hide-modules --config conf/webpack.product.config.babel.js",
    "unitest": "cross-env PRODUCT=1 && cross-env UNITEST=1 ./node_modules/karma/bin/karma start",
    "release": "npm run unitest --verbose && npm run product --verbose",
    "test": "eslint ./conf/* ./bin/libs/* && mocha --require babel-polyfill --compilers js:babel-register unitest/builder/",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --require babel-polyfill --compilers js:babel-register --report coverage/ --recursive unitest/builder/ --colors",
    "coveralls": "npm run coverage --report lcovonly && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start": "npm run develop"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.1.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babel-runtime": "^5.8.0",
    "browser-sync": "^2.15.0",
    "browser-sync-webpack-plugin": "^1.1.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "columnify": "^1.5.4",
    "commander": "^2.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "eslint-loader": "^1.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.6",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.30.0",
    "handlebars": "^4.0.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "image-webpack-loader": "^1.8.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jquery": "^3.1.0",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.14.0",
    "mocha": "^3.0.2",
    "ng-annotate-loader": "^0.1.1",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.5",
    "spritesheet-templates": "^10.1.4",
    "style-loader": "^0.13.1",
    "stylelint": "^7.0.1",
    "stylelint-loader": "^6.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1",
    "webpack-spritesmith": "^0.2.0",
    "webpack-svgstore-plugin": "^3.0.6"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "angular-resource": "^1.5.7",
    "angular-sanitize": "^1.5.6",
    "angular-touch": "^1.5.7",
    "angular-ui-router": "^0.2.15",
